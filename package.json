    "test_programmes": "protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @programmes\"",
    "preview_programmes": "cross-env TEST_ENV=preview protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @programmes\"",
    "live_programmes": "cross-env TEST_ENV=live protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @programmes\"",
    "test_applicants": "protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @applicants\"",
