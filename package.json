    "test_debug": "protractor typeScript/config/config.js --cucumberOpts.tags=\"@debug\"",
    "test_learneradmin": "cross-env TEST_ENV=qa protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @learneradmin\"",
    "preview_learneradmin": "cross-env TEST_ENV=preview protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @learneradmin\"",
