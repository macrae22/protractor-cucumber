  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cucumber": "^2.0.4",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^8.0.3",
    "@types/selenium-webdriver": "^3.0.10",
    "browserstack-local": "^1.3.3",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "chromedriver": "^2.40.0",
    "cross-env": "^5.2.0",
    "cucumber": "^3.0.4",
    "cucumber-html-reporter": "^3.0.4",
    "cucumber-junit": "^1.7.1",
    "cucumber-pretty": "^1.4.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "protractor": "^5.3.2",
    "protractor-cucumber-framework": "^4.0.8",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.6",
    "ts-node": "^3.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
