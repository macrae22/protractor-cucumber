    "preview_applicants": "cross-env TEST_ENV=preview protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @applicants\"",
    "test_applicantsCA": "protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @applicantsCA\"",
    "preview_applicantsCA": "cross-env TEST_ENV=preview protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @applicantsCA\"",
    "live_applicantsCA": "cross-env TEST_ENV=live protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @applicantsCA\"",
    "test_learners": "cross-env TEST_ENV=qa protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @learners\"",
    "test_learningportal": "protractor typeScript/config/config.js --cucumberOpts.tags=\"not @ignore and @learningportal\"",
