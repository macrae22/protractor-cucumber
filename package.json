    "protractor": "protractor",
    "test": "protractor typeScript/config/config.js --cucumberOpts.tags \"not @ignore\"",
    "test_dev": "cross-env TEST_ENV=dev protractor typeScript/config/config.js --cucumberOpts.tags \"not @ignore\"",
